{
  "name": "quickstart",
  "author": "@piprees",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "predev": "yarn install && yarn audit",
    "dev": "yarn concurrently -n next,storybook,jest,cypress -c green,magenta,cyan,yellow --kill-others --restart-tries 0  --handle-input --default-input-target 2 \"yarn next dev\" \"yarn storybook:watch\" \"yarn test:unit:json:watch\" \"yarn test:e2e:watch\"",
    "prebuild": "yarn storybook",
    "build": "next build",
    "start": "yarn next start",
    "lint": "yarn lint:css && yarn lint:md && yarn lint:js && yarn lint:types",
    "lint:css": "yarn stylelint \"./**/*.css\" --fix",
    "lint:js": "yarn eslint --fix --ext ts --ext tsx --ext js --ext jsx --ext json .",
    "lint:types": "yarn tsc --pretty --noEmit",
    "lint:md": "markdownlint \"./**/*.md\" --ignore node_modules --fix",
    "test": "yarn test:unit && yarn test:e2e",
    "test:watch": "yarn test:unit:watch && yarn test:e2e:watch",
    "test:unit": "yarn jest",
    "test:unit:watch": "yarn test:unit --watch",
    "test:unit:json": "jest --json --outputFile=.storybook/.jest-test-results.json || true",
    "test:unit:json:watch": "jest --json --outputFile=.storybook/.jest-test-results.json --watch  || true",
    "test:e2e": "yarn test:e2e:run && yarn test:e2e:merge && yarn test:e2e:html",
    "test:e2e:watch": "yarn cypress open",
    "pretest:e2e:run": "yarn clean:results",
    "test:e2e:run": "yarn start-server-and-test \"next dev\" http://localhost:3000 \"yarn cypress run\"",
    "test:e2e:merge": "npx mochawesome-merge \"./.cypress/__reports__/*.json\" > \"./.storybook/.cypress-test-results.json\"",
    "test:e2e:html": "yarn marge --reportDir \"./public/cypress/\" \"./.storybook/.cypress-test-results.json\"",
    "test:json": "yarn test:unit:json && yarn test:e2e",
    "commit": "yarn lint",
    "clean": "yarn clean:cache && yarn clean:nextcache && yarn clean:storybook",
    "clean:cache": "rm -rf ./node_modules/.cache || true",
    "clean:nextcache": "rm -rf ./.next || true",
    "clean:storybook": "rm -rf ./public/storybook || true",
    "clean:results": "rm -rf ./.cypress/results || true",
    "prestorybook": "yarn clean:storybook && yarn test:json",
    "storybook": "yarn build-storybook --loglevel warn --quiet --output-dir ./public/storybook",
    "storybook:watch": "yarn start-storybook -p 6006 -s ./public --loglevel warn --quiet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn commit"
    }
  },
  "browserslist": "> 0.5% in my stats",
